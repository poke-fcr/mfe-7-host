{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"host": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/host",
			"sourceRoot": "projects/host/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/host",
						"index": "projects/host/src/index.html",
						"main": "projects/host/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/host/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/host/src/favicon.ico",
							"projects/host/src/assets"
						],
						"styles": [
							"projects/host/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/host/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/host/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "projects/host/src/environments/environment.ts",
									"with": "projects/host/src/environments/environment.development.ts"
								}
							]
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "host:build:production",
							"extraWebpackConfig": "projects/host/webpack.prod.config.js",
							"proxyConfig": "projects/host.proxy.conf.prod.json"
						},
						"development": {
							"browserTarget": "host:build:development",
							"proxyConfig": "projects/host/proxy.conf.json"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/host/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "host:build",
						"extraWebpackConfig": "projects/host/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/host/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/host/src/favicon.ico",
							"projects/host/src/assets"
						],
						"styles": [
							"projects/host/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"remote-1": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/remote-1",
			"sourceRoot": "projects/remote-1/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/remote-1",
						"index": "projects/remote-1/src/index.html",
						"main": "projects/remote-1/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/remote-1/tsconfig.app.json",
						"assets": [
							"projects/remote-1/src/favicon.ico",
							"projects/remote-1/src/assets"
						],
						"styles": [
							"projects/remote-1/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/remote-1/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
						"deployUrl": "https://mfe-7-remote-1.vercel.app/",
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/remote-1/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "remote-1:build:production",
							"extraWebpackConfig": "projects/remote-1/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "remote-1:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/remote-1/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "remote-1:build",
						"extraWebpackConfig": "projects/remote-1/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/remote-1/tsconfig.spec.json",
						"assets": [
							"projects/remote-1/src/favicon.ico",
							"projects/remote-1/src/assets"
						],
						"styles": [
							"projects/remote-1/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	}
}